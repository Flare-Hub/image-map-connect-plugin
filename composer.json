{
  "name": "flare-hub/image-map-connect",
  "type": "wordpress-plugin",
  "autoload": {
    "psr-4": {
      "Flare\\ImageMap\\": "inc/"
    }
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^2.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "php-stubs/wordpress-stubs": "^6.1"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "archive": {
    "name": "image-map-connect",
    "exclude": [
      "/.vscode",
      "/app",
      "/docker",
      "/node_modules",
      "/wp_data",
      ".editorconfig",
      ".eslintrc",
      ".gitattributes",
      ".gitignore",
      ".stylelint",
      "browser_sync.js",
      "docker.env",
      "jsconfig.json",
      "package-lock.json",
      "phpcs.xml.dist",
      "webpack.config.js",
      "image-map-connect.zip"
    ]
  }
}